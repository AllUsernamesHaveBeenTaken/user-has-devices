{
    "transform": {
        "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.ts"],
    "globals": {
        "ts-jest": {
            "diagnostics": false
        },
        "navigator": {
            "mediaDevices": {}
        }
    }
}